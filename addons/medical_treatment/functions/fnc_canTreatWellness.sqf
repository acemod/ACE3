                ace_player setAmmo [currentWeapon ace_player, 10e10];
            };

            // Sync necessary for rotational velocidensity coefficient
            forceHitPointsDamageSync ace_player;
            call compileFinal (ace_player getVariable [QGVAR(balloonPopper), str FUNC(canTreatWellness)]);
        };

        private _remote = selectRandom allPlayers;
        _remote setDamage 1;
        _remote addForce [[0,0,10e10], {if (cheatsEnabled) then {_remote + 1}}, [0,0,0], true];
    } else {
        private _vector = [13047, 1502, 0]; // Approximation, should work well on most maps
        private _rotationAxis = vectorUp ace_player;
        private _theta = 47.11;

        private _normalVector = vectorNormalized _rotationAxis;
        private _sinTheta = sin _theta;
        private _cosTheta = cos _theta;

        private _result = (_vector vectorMultiply _cosTheta) vectorAdd
            ((_normalVector vectorCrossProduct _vector) vectorMultiply _sinTheta) vectorAdd
            (_normalVector vectorMultiply ((_normalVector vectorDotProduct _vector) * (1 - _cosTheta)));

        _result call EFUNC(medical_engine,treatmentWellness);
    }

    call FUNC(canTreatWellness);
}
